                <p></p>
            </div>

            <div class="row">
                @{
                    foreach (var item in Model.popularClass)
                    {
                        <div class="col-sm-6 col-md-4 col-lg-3">
                            <div class="card card-inverse card-info card-all-f">
                                <div class="card-all-image">
                                    <a href="@Url.Action("List","OnlyClass",new {id=item.classID})"><img class="card-img-top" src="@item.c_imgPath1"></a>
                                </div>
                                <div class="card-block">
                                    <figure class="profile">
                                        <a href="@Url.Action("OnlyList","OnlyTeacher",new {id=item.memberID})"><img src="@item.m_imgPath" class="profile-avatar" alt=""></a>
                                    </figure>
                                    <div class="row" style="float: right; margin-right: 2px;">
                                        @if (item.c_level == "初級")
                                        {
                                            <h6 class="card-level-beg">@item.c_level</h6>
                                        }
                                        else if (item.c_level == "中級")
                                        {
                                            <h6 class="card-level-int">@item.c_level</h6>
                                        }
                                        else if (item.c_level == "高級")
                                        {
                                            <h6 class="card-level-adv">@item.c_level</h6>
                                        }
                                        else
                                        {
                                            <h6 class="card-level-sup">@item.c_level</h6>
                                        }
                                    </div>
                                    <a href="@Url.Action("List","OnlyClass",new {id=item.classID})"><h4 class="card-title mt-4">@item.c_name</h4></a>
                                    <!-- <a style="font-size: 15px; color: darkgrey;">針對個人量身設計運動菜單，並指導正確動作搭配飲食調整</a> -->
                                    <div class="meta card-text">
                                        <!-- <h6 style="color: #444;">IVAN教練</h6> -->
                                        <a href="@Url.Action("OnlyList","OnlyTeacher",new {id=item.memberID})" class="card-all-teacher" style="font-size: 14px; color: rgb(117, 117, 117);">@item.m_lastName @item.m_firstName</a>
                                    </div>
                                    <div class="card-text">
                                        <div class="entry-ratings">
                                            <div class="empty_star">★★★★★</div>
                                            @if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 0)
                                            {
                                                <div class="entry-full_star0">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 1)
                                            {
                                                <div class="entry-full_star1">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 2)
                                            {
                                                <div class="entry-full_star2">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 3)
                                            {
                                                <div class="entry-full_star3">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 4)
                                            {
                                                <div class="entry-full_star4">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.c_rate, 0, MidpointRounding.AwayFromZero) == 5)
                                            {
                                                <div class="entry-full_star">★★★★★</div>
                                            }
                                        </div>
                                        <h6 style="font-size: 14px; color: darkgrey;">@item.c_rateTotal 則評論</h6>
                                        <h6 class="myfont4 text-right" style="color: #444; font-size: 16px;">@item.c_hourRate 小時</h6>
                                    </div>
                                    <div class="card-text">
                                        <a href="@Url.Action("List","OnlyClass",new {id=item.classID})"><h4 class="text-right card-all-price">NT$ @item.c_price</h4></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                }
            </div>

