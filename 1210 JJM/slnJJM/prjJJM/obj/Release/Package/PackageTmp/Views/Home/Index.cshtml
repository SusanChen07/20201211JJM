                                            else if (Math.Round((double)item.t_rateAvg, 0, MidpointRounding.AwayFromZero) == 2)
                                            {
                                                <div class="entry-full_star2">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.t_rateAvg, 0, MidpointRounding.AwayFromZero) == 3)
                                            {
                                                <div class="entry-full_star3">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.t_rateAvg, 0, MidpointRounding.AwayFromZero) == 4)
                                            {
                                                <div class="entry-full_star4">★★★★★</div>
                                            }
                                            else if (Math.Round((double)item.t_rateAvg, 0, MidpointRounding.AwayFromZero) == 5)
                                            {
                                                <div class="entry-full_star">★★★★★</div>
                                            }
                                        </div>
                                        <br>
                                        <!-- <a href="#" class="btn btn-success mb-4">詳細資訊</a>  -->
                                    </div>
                                </div>
                            }
                        }
                    </div>
                </div>
